 				 u16 dev_attrtype, struct genl_info *info,
 				 void **ehdrp);
 
+#if BITS_PER_LONG == 64 && defined(__BIG_ENDIAN)
+void ethnl_bitmap_to_u32(unsigned long *bitmap, unsigned int nwords);
+#else
+static inline void ethnl_bitmap_to_u32(unsigned long *bitmap,
+				       unsigned int nwords)
+{
+}
+#endif
+
+int ethnl_bitset_is_compact(const struct nlattr *bitset, bool *compact);
+int ethnl_bitset_size(bool compact, unsigned int size, const unsigned long *val,
+		      const unsigned long *mask, const void *names,
+		      bool legacy);
+int ethnl_bitset32_size(bool compact, unsigned int size, const u32 *val,
+			const u32 *mask, const void *names, bool legacy);
+int ethnl_put_bitset(struct sk_buff *skb, int attrtype, bool compact,
+		     unsigned int size, const unsigned long *val,
+		     const unsigned long *mask, const void *names, bool legacy);
+int ethnl_put_bitset32(struct sk_buff *skb, int attrtype, bool compact,
+		       unsigned int size, const u32 *val, const u32 *mask,
+		       const void *names, bool legacy);
+bool ethnl_update_bitset(unsigned long *bitmap, unsigned long *bitmask,
+			 unsigned int nbits, const struct nlattr *attr,
